{
  "name": "portal-vue",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "start": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "vee-validate": "^2.1.7",
    "vue": "^2.5.17",
    "vuetify": "^1.2.3"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/polyfill": "^7.0.0-beta.49",
    "@vue/cli-plugin-babel": "^3.0.0-rc.10",
    "@vue/cli-plugin-eslint": "^3.0.0-rc.10",
    "@vue/cli-service": "^3.0.0-rc.10",
    "axios": "^0.18.0",
    "babel-plugin-transform-imports": "^1.4.1",
    "font-awesome": "^4.7.0",
    "jquery": "^3.3.1",
    "moment": "^2.22.2",
    "node-sass": "^4.10.0",
    "sass-loader": "^7.1.0",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "vue-cli-plugin-vuetify": "^0.1.6",
    "vue-loader": "^15.4.2",
    "vue-notification": "^1.3.13",
    "vue-quill-editor": "^3.0.6",
    "vue-router": "^3.0.1",
    "vue-template-compiler": "^2.5.17",
    "vue-tinymce-editor": "^1.6.2",
    "vuex": "^3.0.1",
    "vuex-router-sync": "^5.0.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {
      "no-unused-vars": 0,
      "no-console": "off",
      "indent": 0,
      "quotes": 0,
      "linebreak-style": 0,
      "semi": 0,
      "space-before-function-paren": 0,
      "no-useless-escape": 0
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "node": {
    "console": "empty",
    "fs": "empty",
    "net": "empty",
    "tls": "empty"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
