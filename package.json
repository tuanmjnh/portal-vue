{
  "name": "portal-vue",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "start": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "buildp": "vue-cli-service build --production",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "vee-validate": "^2.1.7",
    "vue": "^2.6.8",
    "vuetify": "^1.5.5"
  },
  "devDependencies": {
    "@babel/core": "^7.3.4",
    "@babel/polyfill": "^7.2.5",
    "@vue/cli-plugin-babel": "^3.4.1",
    "@vue/cli-plugin-eslint": "^3.4.1",
    "@vue/cli-service": "^3.6.0",
    "axios": "^0.18.0",
    "babel-plugin-transform-imports": "^1.5.1",
    "jquery": "^3.4.0",
    "moment": "^2.24.0",
    "node-sass": "^4.11.0",
    "sass-loader": "^7.1.0",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "vue-cli-plugin-vuetify": "^0.5.0",
    "vue-loader": "^15.7.0",
    "vue-notification": "^1.3.16",
    "vue-quill-editor": "^3.0.6",
    "vue-router": "^3.0.2",
    "vue-template-compiler": "^2.6.8",
    "vuex": "^3.1.0",
    "vuex-router-sync": "^5.0.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {
      "no-unused-vars": 0,
      "no-console": "off",
      "indent": 0,
      "quotes": 0,
      "linebreak-style": 0,
      "semi": 0,
      "space-before-function-paren": 0,
      "no-useless-escape": 0,
      "no-redeclare": 0,
      "vue/no-unused-vars": 1
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "node": {
    "console": "empty",
    "fs": "empty",
    "net": "empty",
    "tls": "empty"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
